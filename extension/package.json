{
  "name": "superdocs",
  "displayName": "superdocs",
  "description": "Agentic programming assistant",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Superdocs",
      "properties": {
        "superdocs.telemetryAllowed": {
          "type": "boolean",
          "default": true,
          "description": "Allow telemetry"
        }
      }
    },
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "superdocs.superdocsView",
          "name": "Superdocs"
        }
      ]
    },
    "commands": [
      {
        "command": "superdocs.addSnippet",
        "title": "Superdocs: Add Snippet"
      },
      {
        "command": "superdocs.sendDirectory",
        "title": "Superdocs: Send Directory to Frontend"
      },
      {
        "command": "superdocs.sendTerminal",
        "title": "Superdocs: Add Terminal Content"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "superdocs.addSnippet",
          "when": "editorHasSelection"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p src",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@mantine/notifications": "^7.3.2",
    "axios": "^1.6.0",
    "brx-node": "^0.4.8-development",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "posthog-js": "^1.131.4",
    "socket.io": "^4.7.2"
  }
}
